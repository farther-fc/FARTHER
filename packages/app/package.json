    "env:dev": "dotenv -e ../../.env -- ",
    "env:prod": "dotenv -e ../../.env.production -- ",
    "env:staging": "dotenv -e ../../.env.staging -- ",
    "dev": "next dev",
    "codegen": "pnpm env:prod graphclient build && ts-node lib/codegenPostScript.ts",
    "graph:dev": "dotenv -e ../../.env.staging graphclient serve-dev",
    "build:web": "pnpm next build",
    "start": "pnpm codegen && next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "typecheck": "tsc --noEmit",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "tipsAgentModeling": "pnpm env:dev ts-node server/tips/agentModeling",
    "test": "cd ../backend && pnpm db:reset && cd ../app && dotenv -e ../../.env jest"
  },
