{
  "name": "@farther/backend",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "env:dev": "dotenv -e ../../.env -- ",
    "env:staging": "dotenv -e ../../.env.staging -- ",
    "env:prod": "dotenv -e ../../.env.production -- ",
    "getTweet": "pnpm env:prod ts-node src/scripts/getTweet.ts",
    "getLpInfo:prod": "pnpm env:prod ts-node src/scripts/getLpInfo.ts",
    "prepareLpBonusDrop:prod": "pnpm env:prod ts-node -T src/scripts/prepareLpBonusDrop.ts",
    "prepareLpBonusDrop:staging": "pnpm env:staging ts-node -T src/scripts/prepareLpBonusDrop.ts",
    "preparePowerDrop:prod": "pnpm env:prod ts-node src/scripts/preparePowerDrop.ts",
    "preparePowerDrop:staging": "pnpm env:staging ts-node src/scripts/preparePowerDrop.ts",
    "preparePowerDrop:dev": "pnpm env:dev ts-node src/scripts/preparePowerDrop.ts",
    "prepareEvangelistDrop:prod": "pnpm env:prod ts-node src/scripts/prepareEvangelistDrop.ts",
    "prepareEvangelistDrop:staging": "pnpm env:staging ts-node src/scripts/prepareEvangelistDrop.ts",
    "prepareEvangelistDrop:dev": "pnpm env:dev ts-node src/scripts/prepareEvangelistDrop.ts",
    "prepareTipsDrop:prod": "pnpm env:prod ts-node src/scripts/prepareTipsDrop.ts",
    "prepareTipsDrop:staging": "pnpm env:staging ts-node src/scripts/prepareTipsDrop.ts",
    "prepareTipsDrop:dev": "pnpm env:dev ts-node src/scripts/prepareTipsDrop.ts",
    "liquidityTracker:prod": "pnpm env:prod ts-node src/scripts/liquidityTracker.ts",
    "getAirdropData:prod": "pnpm env:prod ts-node src/scripts/getAirdropData.ts",
    "verifyAirdrop:prod": "pnpm env:prod ts-node src/scripts/verifyAirdrop.ts",
    "migrate:create": "pnpm env:dev prisma migrate dev",
    "migrate:staging": "pnpm env:staging prisma migrate deploy",
    "migrate:prod": "pnpm env:prod prisma migrate deploy",
    "generate": "prisma generate",
    "db:push": "pnpm env:dev prisma db push --skip-generate",
    "db:reset": "pnpm env:dev ts-node src/scripts/reset.ts",
    "db:format": "prisma format",
    "db:view": "pnpm env:dev prisma studio",
    "dev": "pnpm db:push && pnpm generate",
    "start": "pnpm env:dev pnpm ts-node src/server",
    "start:staging": "pnpm env:staging pnpm ts-node src/server --transpile-only",
    "start:prod": "pnpm env:prod pnpm ts-node src/server --transpile-only",
    "updateTipperScores": "CRON=updateTipperScores ts-node src/crons.ts --transpile-only",
    "updateEligibleTippers": "CRON=updateEligibleTippers ts-node src/crons.ts --transpile-only",
    "distributeAllowances": "CRON=distributeAllowances ts-node src/crons.ts --transpile-only",
    "takeOpenRankSnapshot": "CRON=takeOpenRankSnapshot ts-node src/crons.ts --transpile-only",
    "test": "pnpm db:reset && pnpm env:dev jest"
  },
  "prisma": {
    "schema": "./src/prisma/schema.prisma"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@airstack/node": "^0.0.7",
    "@farther/common": "*",
    "@neondatabase/serverless": "^0.7.2",
    "@neynar/nodejs-sdk": "^1.19.3",
    "@prisma/client": "^5.1.0",
    "@sentry/node": "^8.15.0",
    "@sentry/profiling-node": "^8.15.0",
    "ajv": "^8.13.0",
    "bluebird": "^3.7.2",
    "bullmq": "^5.10.0",
    "d3": "^6.7",
    "dayjs": "^1.11.11",
    "decimal.js": "^10.4.3",
    "enquirer": "^2.4.1",
    "ethers": "^6.11.1",
    "ioredis": "^5.4.1",
    "jest": "^29.7.0",
    "later": "^1.2.0",
    "node-cron": "^3.0.3",
    "numeral": "^2.0.6",
    "pg": "^8.11.3",
    "require-env-variable": "^4.0.2",
    "ts-jest": "^29.1.2",
    "underscore": "^1.13.6",
    "uuid": "^9.0.1",
    "viem": "2.10.2"
  },
  "devDependencies": {
    "@prisma/client": "^5.1.0",
    "@types/bluebird": "^3.5.42",
    "@types/d3": "^7.4.3",
    "@types/jest": "^29.5.12",
    "@types/later": "^1.2.9",
    "@types/node": "^20.11.3",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.10.9",
    "@types/underscore": "^1.11.15",
    "@types/uuid": "^9.0.8",
    "dotenv": "^16.3.2",
    "esbuild-register": "^3.5.0",
    "prisma": "^5.8.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  }
}
