{
  "name": "protocol",
  "version": "1.0.0",
  "description": "",
  "exports": "./index.js",
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "env:dev": "dotenvx run -f ../../.env -- ",
    "env:staging": "dotenvx run -f ../../.env.staging -- ",
    "env:prod": "dotenvx run -f../../.env.production -- ",
    "anvil": "anvil --mnemonic \"question federal analyst minimum outdoor clarify canoe cream relax poet thing embrace\"",
    "dev:deploy:token": "pnpm env:dev forge script script/DeployToken.s.sol:DeployToken --rpc-url http://127.0.0.1:8545 --private-key $DEPLOYER_PK -vvvvv --broadcast",
    "deploy:token": "forge script script/DeployToken.s.sol:DeployToken --rpc-url $NEXT_PUBLIC_BASE_RPC_URL --private-key $DEPLOYER_PK --verifier-url https://api.basescan.org/api --verify -vvvvv --broadcast",
    "deploy:airdrop": "forge script script/DeployAirdrop.s.sol:DeployAirdrop --rpc-url $NEXT_PUBLIC_BASE_RPC_URL --private-key $DEPLOYER_PK --verifier-url https://api.basescan.org/api --verify -vvvvv --broadcast",
    "deploy:dao": "forge script script/DeployDAO.s.sol:DeployDAO --rpc-url $NEXT_PUBLIC_BASE_RPC_URL --private-key $DEPLOYER_PK --verifier-url https://api.basescan.org/api --verify -vvvvv --broadcast",
    "propose": "forge script script/TestProposal.s.sol:Propose --rpc-url $NEXT_PUBLIC_BASE_RPC_URL --private-key $DEPLOYER_PK --verifier-url https://api.basescan.org/api --verify -vvvvv --broadcast",
    "execute": "forge script script/TestProposal.s.sol:Execute --rpc-url $NEXT_PUBLIC_BASE_RPC_URL --private-key $DEPLOYER_PK --verifier-url https://api.basescan.org/api --verify -vvvvv --broadcast",
    "vote": "forge script script/Vote.s.sol:Vote --rpc-url $NEXT_PUBLIC_BASE_RPC_URL --private-key $DEPLOYER_PK --verifier-url https://api.basescan.org/api --verify -vvvvv --broadcast",
    "staging:deploy:token": "pnpm env:staging pnpm deploy:token",
    "prod:deploy:token": "pnpm env:prod pnpm deploy:token",
    "staging:deploy:airdrop": "pnpm env:staging pnpm deploy:airdrop",
    "prod:deploy:airdrop": "pnpm env:prod pnpm deploy:airdrop",
    "staging:deploy:dao": "TOKEN_ADDRESS=\"0xf9A98fDC95A427fCfB1506A6E8A3143119417fBA\" VOTING_DELAY=0 VOTING_PERIOD=150 PROPOSAL_THRESHOLD=0x152D02C7E14AF6800000 pnpm env:staging pnpm deploy:dao",
    "prod:deploy:dao": "TOKEN_ADDRESS=\"0x8ad5b9007556749DE59E088c88801a3Aaa87134B\" VOTING_DELAY=7200 VOTING_PERIOD=50400 PROPOSAL_THRESHOLD=0x152D02C7E14AF6800000 pnpm env:prod pnpm deploy:dao",
    "staging:propose": "pnpm env:staging pnpm propose",
    "staging:execute": "pnpm env:staging pnpm execute",
    "foundry": "curl -L https://foundry.paradigm.xyz | bash && source /vercel/.bashrc && foundryup && forge install",
    "typechain": "typechain --target ethers-v6 --out-dir ../common/src/typechain ./out/**/*.json",
    "test": "pnpm env:dev forge test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@typechain/ethers-v6": "^0.5.1",
    "@wagmi/cli": "^2.1.4",
    "dotenv": "^16.3.2",
    "solhint": "^3.6.2",
    "typechain": "^8.3.2",
    "viem": "2.10.2",
    "wagmi": "^2.5.11"
  }
}
