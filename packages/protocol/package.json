{
  "name": "protocol",
  "version": "1.0.0",
  "description": "",
  "exports": "./index.js",
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "env:staging": "dotenv -e ../../.env.staging -- ",
    "env:prod": "dotenv -e ../../.env.production -- ",
    "anvil": "anvil --mnemonic \"question federal analyst minimum outdoor clarify canoe cream relax poet thing embrace\"",
    "dev:deploy:token": "source ../../.env && forge script script/DeployToken.s.sol:DeployToken --rpc-url http://127.0.0.1:8545 --private-key $DEPLOYER_PK -vvvvv --broadcast",
    "deploy:token": "ETHERSCAN_API_KEY=3JYZ4DQKMQ5W4NJ89IBQI45UWDKYK3IM2B forge script script/DeployToken.s.sol:DeployToken --rpc-url $BASE_RPC_URL --private-key $DEPLOYER_PK --verifier-url https://api.basescan.org/api --verify -vvvvv --broadcast",
    "deploy:airdrop": "ETHERSCAN_API_KEY=3JYZ4DQKMQ5W4NJ89IBQI45UWDKYK3IM2B forge script script/DeployAirdrop.s.sol:DeployAirdrop --rpc-url $BASE_RPC_URL --private-key $DEPLOYER_PK --verifier-url https://api.basescan.org/api --verify -vvvvv --broadcast",
    "deploy:vesting": "ETHERSCAN_API_KEY=3JYZ4DQKMQ5W4NJ89IBQI45UWDKYK3IM2B forge script script/DeployVesting.s.sol:DeployVesting --rpc-url $BASE_RPC_URL --private-key $DEPLOYER_PK --verifier-url https://api.basescan.org/api --verify -vvvvv --broadcast",
    "staging:deploy:token": "pnpm env:staging pnpm deploy:token",
    "prod:deploy:token": "pnpm env:prod pnpm deploy:token",
    "staging:deploy:airdrop": "pnpm env:staging pnpm deploy:airdrop",
    "prod:deploy:airdrop": "pnpm env:prod pnpm deploy:airdrop",
    "staging:deploy:vesting": "pnpm env:staging pnpm deploy:vesting",
    "prod:deploy:vesting": "pnpm env:prod pnpm deploy:vesting",
    "foundry": "curl -L https://foundry.paradigm.xyz | bash && source /vercel/.bashrc && foundryup && forge install",
    "generate": "wagmi generate",
    "typechain": "typechain --target ethers-v6 --out-dir ../common/src/typechain ./out/**/*.json",
    "test": "dotenv -e ../../.env forge test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@typechain/ethers-v6": "^0.5.1",
    "@wagmi/cli": "^2.1.4",
    "dotenv": "^16.3.2",
    "typechain": "^8.3.2",
    "viem": "2.10.2",
    "wagmi": "^2.5.11"
  }
}
