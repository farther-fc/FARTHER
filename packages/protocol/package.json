{
  "name": "protocol",
  "version": "1.0.0",
  "description": "",
  "exports": "./index.js",
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "anvil": "anvil --mnemonic \"question federal analyst minimum outdoor clarify canoe cream relax poet thing embrace\"",
    "deploy:dev": "source ../../.env && forge script script/DeployProtocol.s.sol:Deploy --rpc-url http://127.0.0.1:8545 --private-key $PRIVATE_KEY -vvvvv --broadcast",
    "token:staging": "source ../../.env.staging && forge script script/DeployToken.s.sol:DeployToken --rpc-url $SEPOLIA_RPC_URL --private-key $PRIVATE_KEY --verify -vvvv --broadcast",
    "airdrop:staging": "source ../../.env.staging && forge script script/DeployAirdrop.s.sol:DeployAirdrop --rpc-url $SEPOLIA_RPC_URL --private-key $PRIVATE_KEY --verify -vvvv --broadcast",
    "foundry": "curl -L https://foundry.paradigm.xyz | bash && source /vercel/.bashrc && foundryup && forge install",
    "generate": "wagmi generate",
    "typechain": "typechain --target ethers-v6 --out-dir ../common/src/typechain ./out/**/*.json"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@typechain/ethers-v6": "^0.5.1",
    "@wagmi/cli": "^2.1.4",
    "dotenv": "^16.3.2",
    "typechain": "^8.3.2",
    "viem": "^2.8.14",
    "wagmi": "^2.5.11"
  }
}
